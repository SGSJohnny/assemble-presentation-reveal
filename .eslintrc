{
  "parser": "babel-eslint",
  "ecmaFeatures": {
        "blockBindings": true,
        "forOf": true,
        "jsx": true,
        "classes": true,
        "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "es6": true
  },
  "globals": {
    "__DEV__": true,
    "__SERVER__": true,
    "require": true,
    "process": true
  },
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "single"]
  }
}
